<xsd:schema version="1"
  xmlns:ob="http://www.ondalear.com/core/2011-05"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://www.ondalear.com/core/2011-05"
  elementFormDefault="qualified">
  <xsd:annotation>
    <xsd:documentation>
      <dl>
        <dt>Defines the schema for core module.</dt>
        <dt>ob - Ondalear Core/Base  name space.</dt>
        <dt>Strives to conform  to best practices defined inhttp://xml.coverpages.org/HP-StephensonSchemaBestPractices.pdf</dt>
      </dl>
    </xsd:documentation>
  </xsd:annotation>




  <xsd:simpleType name="BooleanType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="true"/>
      <xsd:enumeration value="false"/>
    </xsd:restriction>
  </xsd:simpleType>

  
  <xsd:simpleType name="StringType">
    <xsd:restriction base="xsd:string">
    </xsd:restriction>
  </xsd:simpleType>
  

  
</xsd:schema>